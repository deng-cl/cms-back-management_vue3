{
    "extends": "@vue/tsconfig/tsconfig.dom.json",
    "include": [
        "env.d.ts",
        "custom.d.ts",
        "src/**/*",
        "src/**/*.ts",
        "src/**/*.vue",
        "element-plus/dist/locale/zh-cn.mjs"
    ],
    "exclude": [
        "src/**/__tests__/*"
    ],
    "compilerOptions": {
        "composite": true,
        "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
        "baseUrl": ".",
        "paths": {
            "@/*": ["src/*"]
        }
    }
}
